     ]
    }
   ],
   "source": [
    "### 定义 2：严格版专业房东（Strict Professional Landlord）\n",
    "### 标准：拥有 >= 3 套整套房源\n",
    "\n",
    "strict_pro = df[df[\"calculated_host_listings_count_entire_homes\"] >= 3]\n",
    "\n",
    "num_strict_pro = strict_pro[\"host_id\"].nunique()\n",
    "num_strict_properties = strict_pro.shape[0]\n",
    "\n",
    "print(\"【定义 B：≥3 整套房源】\")\n",
    "print(\"严格定义专业房东数量（人）:\", num_strict_pro)\n",
    "print(\"严格定义专业房东管理的房源数量:\", num_strict_properties)\n"
   ]
